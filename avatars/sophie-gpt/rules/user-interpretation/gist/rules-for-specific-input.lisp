    2 ((Do I have a question about chemotherapy ?) (Chemotherapy)) (0 :gist)
  1 (0 .ANYTHING 1 you 2 .WANT-GEN 2 .ASK-GEN 0)
    2 ((Do I have a question about chemotherapy ?) (Chemotherapy)) (0 :gist)

  1 (0 .COMFORT-CARE-WORD 0)
    2 *comfort-care-input* (0 :subtree)
